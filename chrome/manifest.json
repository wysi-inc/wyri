{
    "manifest_version": 3,
    "name": "Wyri",
    "description": "Redirects osu.ppy.sh requests to wysi727.com",
    "version": "1.0",
    "permissions": [
        "declarativeNetRequest",
        "declarativeNetRequestFeedback"
    ],
    "host_permissions": [
        "http://osu.ppy.sh/*",
        "https://osu.ppy.sh/*"
    ],
    "declarative_net_request": {
        "rule_resources": [
            {
                "id": "rules",
                "enabled": true,
                "path": "rules.json"
            }
        ]
    }
}
