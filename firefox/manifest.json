{
    "manifest_version": 2,
    "version": "1.2",
    "name": "Wyri",
    "description": "When You Redirect It redirects osu.ppy.sh requests to wysi727.com",
    "permissions": [
        "storage",
        "webRequest",
        "webRequestBlocking",
        "*://osu.ppy.sh/*"
    ],
    "background": {
        "scripts": [
            "redirect.js"
        ]
    },
    "icons": {
        "16": "icons/icon-16.ico",
        "24": "icons/icon-24.ico",
        "32": "icons/icon-32.ico",
        "48": "icons/icon-48.ico",
        "64": "icons/icon-64.ico",
        "96": "icons/icon-96.ico",
        "128": "icons/icon-128.ico"
    },
    "browser_action": {
        "default_icon": {
            "32": "icons/icon-32.ico"
        },
        "default_title": "WYRI"
    }
}
